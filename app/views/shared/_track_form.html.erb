<%= form_for([:user, @interest]) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
<div class="field">
  <%= f.text_field :artist_name,	placeholder: "Which artist should we track...?", class: "ui-autocomplete-input" %>
  <%= f.text_field :track_name,		placeholder: "Which song should we track...?" %>
</div>
  <%= f.submit "Track this!", class: "btn btn-large btn-primary" %>
<% end %>